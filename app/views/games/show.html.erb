<% @game.rounds.each do |round| %>
  <% if round.played_by? current_user %>
    <h3><%= round.question.text %></h3>
    <ul>
      <% round.responses.each do |response| %>
        <li><img src="<%= response.answer.url %> alt="<%= response.answer.text %>"/><img src="<%= response.user.avatar_url %>" class="avatar" alt="<%= response.user.name %>"/></li>
      <% end %>
    </ul>
  <% else %>
    <%= link_to game_round_path(@game, round) do %>
      <%= round.question.text %>
    <% end %>
  <% end %>
<% end %>
